string convertSentence(string s, int n){
	// Write your code here.
	string output="";
	string str[26] = {
      "2","22","222",
      "3","33","333",
      "4","44","444",
      "5","55","555",
      "6","66","666",
      "7","77","777","7777",
      "8","88","888",
      "9","99","999","9999"
    };
	for(int i=0; i<s.size(); i++){
		int index=s[i]-'a';
		output+=str[index];
	}
	return output;
}
